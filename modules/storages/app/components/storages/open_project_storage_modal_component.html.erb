<%= render(
    Primer::Alpha::Dialog.new(id: self.class.dialog_id,
                              title: 'Open project folder modal',
                              data: @data)
) do |dialog| %>
    <% dialog.with_header(display: :none) %>
    <% dialog.with_body(id: self.class.dialog_body_id, p: 2) do %>
        <%= render(self.class::Body.new(@state)) %>
    <% end %>
<% end %>
