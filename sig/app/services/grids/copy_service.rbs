    def self.copy_dependencies: () -> ::Array[untyped]

    def self.widgets_dependency: () -> untyped

    def initialize: (user: untyped, source: untyped, ?contract_class: untyped) -> void

    def set_attributes_params: (untyped _params) -> untyped
  end
end
